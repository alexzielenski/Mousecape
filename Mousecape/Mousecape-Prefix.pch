#ifdef __OBJC__
#define DEFINE_SHARED_INSTANCE_USING_BLOCK(block) \
static dispatch_once_t pred = 0; \
__strong static id _sharedObject = nil; \
dispatch_once(&pred, ^{ \
_sharedObject = block(); \
}); \
return _sharedObject; \

    #import <Cocoa/Cocoa.h>
    #import "MCPreferences.h"
    #import <ReactiveCocoa.h>
    #import <EXTScope.h>
#endif
